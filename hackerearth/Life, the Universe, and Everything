import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner sc=new Scanner(System.in);
        int i=0;
        while(i==0){
          int k=sc.nextInt();
          if(k==42){
            i=1;
           }
          else{
            System.out.println(k);
          }
        }

    }
}
