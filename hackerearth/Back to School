import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        int b=sc.nextInt();
        int c=sc.nextInt();
       System.out.println( (a>=b && a>=c) ? a : ( (b>=c) ? b : c ) );

    }
}
