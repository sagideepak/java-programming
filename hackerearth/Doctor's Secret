import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner sc=new Scanner(System.in);
        int l=sc.nextInt();
        int p=sc.nextInt();

        System.out.println( ( (l<=23) &&  ( p>=500 && p<=1000) ) ? "Take Medicine" : "Don't take Medicine" );

         

    }
}
