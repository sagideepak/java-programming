import java.util.*;
import java.lang.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        	 Scanner sc=new Scanner(System.in);
        int N=sc.nextInt();
        int c=(int)(Math.pow(10,9));
        long answer=1;
        for(int i=0;i<N;i++){
            int k=sc.nextInt();
            answer=(answer*k)%(c+7);
        }
        System.out.println(answer);


    }
}
