import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int x=sc.nextInt();

        for(int i=0;i<n;i++){
            int y=sc.nextInt();
            if(y>=x){
                System.out.println("YES");
            }
            else{
                System.out.println("NO");
            }
        }

    }
}
