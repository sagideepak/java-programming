import java.util.*;


class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner sc=new Scanner(System.in);
        int l,r,k;
        int count=0;
        l=sc.nextInt();
        r=sc.nextInt();
        k=sc.nextInt();
       for(int i=l;i<=r;i++)
        {
           if(i%k==0){
               count++;
           }
        }

        System.out.println(count);

    }
}
