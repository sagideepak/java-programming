import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner sc=new Scanner(System.in);
        String s=sc.next();
        int result=0;

        for(int i=0;i<s.length();i++){
  
                int k=( s.charAt(i) - 96 );
                result=result+k;

        }
        System.out.println(result);

    }
}
