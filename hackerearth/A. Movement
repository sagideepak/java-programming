import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner sc=new Scanner(System.in);
        int s=sc.nextInt();
        int min=s/5,r=s%5;
        
         if(r!=0){
             min=min+1;
         }
       
        
        System.out.println(min);   



       

    }
}
