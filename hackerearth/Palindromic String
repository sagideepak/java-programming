import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner sc=new Scanner(System.in);
        String s=sc.next();
        String reverse="";
        for(int i=s.length()-1;i>=0;i--){
               reverse=reverse+s.charAt(i);
        }
    
        if(s.equals(reverse)){
            System.out.println("YES");
        }
        else{
             System.out.println("NO");
        }

    }
}
